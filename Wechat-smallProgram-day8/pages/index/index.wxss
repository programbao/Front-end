page,.main{
  width: 100%;
  height: 100%;
}

.list{
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, .5);
  justify-content: center;
  font-size: 76rpx;
}
.content{
  position: fixed;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: #17A8E2;
  z-index: 1;
  transition: all 0.4s;
  
}
.more{
  width: 100rpx;
  position: absolute;
  left: 0;
  top: 0;
  background-color: aqua
}